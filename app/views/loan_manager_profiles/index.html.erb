<p id="notice"><%= notice %></p>

<h1>Loan Managers</h1>

<table>
  <thead>
    <tr>
      <th>Loan manager</th>
      <th>Image</th>
      <th>Sex</th>
      <th>Birth date</th>
      <th>Address</th>
      <th>City</th>
      <th>Zip code</th>
      <th>Civil status</th>
      <th>Mobile</th>
      <th>Company</th>
      <th>Department</th>
      <th>Employee category</th>
      <th>Job title</th>
      <th>Manager</th>
      <th>Enabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @loan_manager_profiles.each do |loan_manager_profile| %>
      <tr>
        <td><%= loan_manager_profile.loan_manager %></td>
        <td><%= loan_manager_profile.image %></td>
        <td><%= loan_manager_profile.sex %></td>
        <td><%= loan_manager_profile.birth_date %></td>
        <td><%= loan_manager_profile.address %></td>
        <td><%= loan_manager_profile.city %></td>
        <td><%= loan_manager_profile.zip_code %></td>
        <td><%= loan_manager_profile.civil_status %></td>
        <td><%= loan_manager_profile.mobile %></td>
        <td><%= loan_manager_profile.company %></td>
        <td><%= loan_manager_profile.department %></td>
        <td><%= loan_manager_profile.employee_category %></td>
        <td><%= loan_manager_profile.job_title %></td>
        <td><%= loan_manager_profile.manager %></td>
        <td><%= loan_manager_profile.enabled %></td>
        <td><%= link_to 'Show', loan_manager_profile %></td>
        <td><%= link_to 'Edit', edit_loan_manager_profile_path(loan_manager_profile) %></td>
        <td><%= link_to 'Destroy', loan_manager_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Loan Manager Profile', new_loan_manager_profile_path %>
