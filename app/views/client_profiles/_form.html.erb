  </div>

  <div class="field">
    <%= form.label :job_title %>
    <%= form.text_field :job_title, id: :client_profile_job_title %>
  </div>

  <div class="field">
    <%= form.label :manager %>
    <%= form.text_field :manager, id: :client_profile_manager %>
  </div>

  <div class="field">
    <%= form.label :enabled %>
    <%= form.check_box :enabled, id: :client_profile_enabled %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
