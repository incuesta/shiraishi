<p id="notice"><%= notice %></p>

<!-- <h1>Admin Profiles</h1> -->


<!-- Panel Table -->
<div class="panel panel-default">

  <div class="panel-heading">Admin Profiles</div>

  <!-- <div class="panel-body">
      <p>Some things to say</p>
  </div>
 -->

    <table class="table table-striped">
      <thead>
        <tr>
          <th>User name</th>
          <th>Image</th>
          <th>Email</th>
          <th>Last Name</th>
          <th>First Name</th>
          <th>Mobile</th>

          <!-- For the Action -->
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @admin_profiles.each do |admin_profile| %>
            <% admin = admin_profile.admin %>
            <tr>
              <td><%= admin.user_name %></td>
              <td><%= admin_profile.image %></td>
              <td><%= admin.email %></td>
              <td><%= admin.last_name %></td>
              <td><%= admin.first_name %></td>
              <td><%= admin_profile.mobile %></td>
             
              <td><%= link_to 'Show', admin_profile %></td>
              <td><%= link_to 'Edit', edit_admin_profile_path(admin_profile) %></td>
              <td><%= link_to 'Destroy', admin_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
        <% end %>
      </tbody>
    </table>

</div>
<!-- Table Panel End -->

<br>


